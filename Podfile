  use_frameworks!

  pod 'SwiftyUtils'
  pod 'GoogleMaps'
  pod 'Eureka'
  pod 'Firebase/Core'
  pod 'Firebase/AdMob'
  pod 'Realm'
  pod 'RealmSwift'
  pod 'SCLAlertView', :git => 'https://github.com/vikmeup/SCLAlertView-Swift.git'
  pod 'RxCocoa'
  pod 'RxSwift'
end
target 'LongLatMapTests' do
    inherit! :search_paths
    pod 'Firebase'
end
